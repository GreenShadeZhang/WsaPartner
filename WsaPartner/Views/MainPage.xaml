<Page
    x:Class="WsaPartner.Views.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    >

    <Grid x:Name="ContentArea" Margin="60,12,60,12">
        <StackPanel>
            <TextBlock Text="ceshi"/>
            <TextBlock Text="{x:Bind ViewModel.Device.Name}"/>
            <TextBlock Text="{x:Bind ViewModel.Device.Features}"/>
            <TextBlock Text="{x:Bind ViewModel.Device.Message}"/>
            <TextBlock Text="{x:Bind ViewModel.Device.Model}"/>
            <TextBlock Text="{x:Bind ViewModel.Device.Product}"/>
            <TextBlock Text="{x:Bind ViewModel.Device.Serial}"/>
            <TextBlock Text="{x:Bind ViewModel.Device.State}"/>
        </StackPanel>
    </Grid>
</Page>
