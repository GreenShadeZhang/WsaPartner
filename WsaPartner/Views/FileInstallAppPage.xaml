<Page
    x:Class="WsaPartner.Views.FileInstallAppPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:WsaPartner.Views"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    xmlns:i="using:Microsoft.Xaml.Interactivity"
    xmlns:ic="using:Microsoft.Xaml.Interactions.Core"
    Style="{StaticResource PageStyle}">
    <i:Interaction.Behaviors>
        <ic:EventTriggerBehavior EventName="Loaded">
            <ic:InvokeCommandAction Command="{x:Bind ViewModel.LoadedCommand}" />
        </ic:EventTriggerBehavior>
    </i:Interaction.Behaviors>
    <Grid>
        <StackPanel>
            <TextBlock>FIle Install App Page</TextBlock>

            <TextBlock Text="{x:Bind ViewModel.AppPath}"></TextBlock>

            <!--<TextBlock Text="{x:Bind ViewModel.TargetPackageData.AppName}" FontSize="32"></TextBlock>-->
                       
            <!--<Image Width="400" Source="{x:Bind ViewModel.AppIcon}"></Image>-->
            <!--<Image Source="{x:Bind ViewModel.IconPath}">
               
            </Image>-->
            <Image x:Name="Icon"></Image>
        </StackPanel>
    </Grid>
</Page>
